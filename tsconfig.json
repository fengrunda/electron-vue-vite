{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@root/*": [
        "./*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "types": [
      // "vite/client",
      // "node",
      "element-plus/global"
    ]
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  // "include": [
  //   // "src/**/*",
  //   "typings/*"
  // ],
  // "exclude": [
  //   "node_modules"
  // ],
}